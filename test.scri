# Variable declaration and assignment
int foo = 21;
foo = foo * 2;

# Short form operators
int i = 10;
i += 2;
print(i);
i -= 2;
print(i);
i *= 4;
print(i);
i /= 2;
print(i);

# Declare a object
obj exampleObj = {
    x: 30,
    y: null,
    z: false,
    bar: foo,
    foo: foo,
};

# Get object properties
int prop = exampleObj.x;
print(prop);
print(exampleObj.x);

# Buildin functions
print(foo, 13);

int timeNow = time();
print(timeNow);

print(time());

# User defined functions
func add(a, b) {
    int result = a + b;
    print(result);
}

print(add);
add(1, 2);

func sub(a, b) {
    int result = 0;
    func add(a, b) {
        result = a + b;
    }

    add(a, b);
    result = result - 2 * b;
    print(result);
}

sub(6, 4);

func addWithReturn(a, b) {
    int result = a + b;
    result;
}

print(addWithReturn(10, 11));